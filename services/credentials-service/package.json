    "fastify-multer": "^2.0.3",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "prisma": "4.8.1",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "uuid": "^9.0.0",
